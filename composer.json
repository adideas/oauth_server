{
    "name": "adideas/oauth_server",
    "description": "Authorization",
    "type": "library",
    "minimum-stability": "dev",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexey Vlasov",
            "email": "adideas@mail.ru"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "firebase/php-jwt": "^5.2",
        "illuminate/auth": "^8.0",
        "illuminate/console": "^8.0",
        "illuminate/cache": "^8.0",
        "illuminate/http": "^8.0",
        "illuminate/validation": "^8.0",
        "illuminate/routing": "8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.8"
    },
    "autoload": {
        "files": [
            "helpers/provider.php"
        ],
        "psr-4": {
            "Adideas\\OauthServer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Adideas\\OauthServer\\Tests\\": "tests/"
        }
    },
    "conflict": {
        "laravel/passport": ">0.*",
        "laravel/sanctum": ">0.*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Adideas\\OauthServer\\Providers\\OauthServiceProvider",
                "Adideas\\OauthServer\\Providers\\RouteServiceProvider"
            ]
        }
    }
}
